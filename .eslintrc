{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": 0,
    "linebreak-style": 0,
    "prefer-template": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/label-has-for": 1,
    "function-paren-newline": ["error", "consistent"],
    "jsx-a11y/anchor-is-valid": 0,
    "no-underscore-dangle": 0,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 100
      }
    ],
    "object-curly-newline": [
      "error",
      {
        "consistent": true
      }
    ]
  },
  "plugins": ["react", "prettier"],
  "env": {
    "es6": true,
    "browser": true
  }
}
